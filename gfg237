/* Node of a linked list
 class Node {
   int data;
    Node next;
    Node(int d)  { data = d;  next = null; }
}
 Linked List class
class LinkedList
{
    Node head;  // head of list
}*/

class Intersect {
    // Function to find intersection point in Y shaped Linked Lists.
    int intersectPoint(Node head1, Node head2) {
       while(head1!=null){
            head1.data+=30000000;
            head1=head1.next;
        }
        while(head2.data<30000000){
            head2=head2.next;
        }
        return head2.data-30000000;
    }
}
